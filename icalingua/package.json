{
  "name": "icalingua",
  "version": "2.2.4-loongarch",
  "author": "clansty <i@gao4.pw>",
  "description": "A client for (maybe) any IM made with Electron",
  "license": "GPL-3.0",
  "main": "./dist/electron/main.js",
  "desktopName": "icalingua.desktop",
  "homepage": "https://github.com/Clansty/Icalingua",
  "scripts": {
    "build": "ts-node .electron-vue/build.ts && electron-builder",
    "build:win": "electron-builder --win",
    "build:arm": "electron-builder --arm",
    "build:electron": "electron-builder",
    "build:ci": "ts-node .electron-vue/build.ts",
    "build:dir": "ts-node .electron-vue/build.ts && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean ts-node .electron-vue/build.ts",
    "dev": "NODE_ENV=development ts-node .electron-vue/dev-runner.ts",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.ts",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.ts"
  },
  "build": {
    "productName": "Icalingua",
    "appId": "com.clansty.icalingua",
    "directories": {
      "output": "build",
      "buildResources": "static"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "tar.xz"
        },
        {
          "target": "deb"
        },
        {
          "target": "rpm"
        },
        {
          "target": "AppImage"
        }
      ],
      "executableName": "icalingua",
      "category": "Network"
    },
    "npmRebuild": false
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/ioredis": "^4.26.4",
    "@types/lovefield": "^2.1.3",
    "@types/lowdb": "^1.0.10",
    "@types/md5": "^2.3.0",
    "@types/minimist": "^1.2.2",
    "@types/mongodb": "^3.6.17",
    "@types/webpack": "^5.28.0",
    "@types/winston": "^2.4.4",
    "ajv": "^6.5.0",
    "aria2": "^4.1.2",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "csv-writer": "^1.6.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "8.5.5",
    "electron-builder": "^22.11.7",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-dl": "^3.2.1",
    "element-ui": "^2.15.1",
    "file-loader": "^1.1.11",
    "file-type": "^16.5.1",
    "freedesktop-notifications": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "linkifyjs": "^2.1.9",
    "lodash": "^4.17.21",
    "lottie-web": "^5.7.11",
    "md5": "^2.3.0",
    "mini-css-extract-plugin": "0.4.0",
    "minimist": "^1.2.5",
    "multispinner": "^0.2.1",
    "noble-ed25519": "^1.2.5",
    "node-loader": "^0.6.0",
    "nodejs-base64": "^2.0.0",
    "oicq": "^1.20.2",
    "sass": "^1.34.1",
    "sass-loader": "^10.1.1",
    "socket.io-client": "^4.1.2",
    "style-loader": "^0.21.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "url-loader": "^1.0.1",
    "v-click-outside": "^3.1.2",
    "v-emoji-picker": "^2.3.3",
    "vue": "^2.6.12",
    "vue-devtools": "^5.1.4",
    "vue-electron": "^1.0.6",
    "vue-emoji-picker": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-infinite-loading": "^2.4.5",
    "vue-loader": "^15.2.4",
    "vue-router": "^3.5.0",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "which": "^2.0.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "yaml": "^1.10.2"
  }
}
